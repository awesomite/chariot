        "symfony/http-foundation": "^3.3.5",
        "symfony/finder": "^3.3.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bart≈Çomiej Krukowski",
            "email": "bartlomiej@krukowski.me"
        }
