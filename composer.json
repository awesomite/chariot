{
    "name": "awesomite/chariot",
    "description": "Just another routing library",
    "keywords": ["routing", "router"],
    "type": "library",
    "require": {
        "php": "^7.0",
        "ext-pcre": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.7 || ^7.0.2",
        "symfony/console": "^3.4.6",
        "symfony/http-foundation": "^3.4.6",
        "symfony/finder": "^3.4.6",
        "behat/behat": "^3.4.3",
        "composer/xdebug-handler": "1.0.0"
    },
    "minimum-stability": "RC",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Bart≈Çomiej Krukowski",
            "email": "bartlomiej@krukowski.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Awesomite\\Chariot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "features/src/",
            "Awesomite\\Chariot\\": "tests/",
            "Awesomite\\Chariot\\Speedtest\\": "speedtest/src/"
        }
    }
}
